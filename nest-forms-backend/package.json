{
  "name": "nest-forms-backend",
  "version": "0.0.1",
  "description": "Backend responsible for processing forms for online services.",
  "author": "Department of Innovation and Technology of the City of Bratislava",
  "private": false,
  "license": "LICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "seed:prod": "node dist/prisma/seed.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,js}\"",
    "lint:report": "eslint -f json -o eslint-report.json \"{src,apps,libs,test}/**/*.{ts,js}\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.{ts,js}\" --fix",
    "test": "jest -w 1",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json",
    "test:send-form": "jest --config test/jest-send-form.json",
    "test:generate-pdf": "jest --config test/jest-generate-pdf.json",
    "vulnerabilities": "npm audit --audit-level=high --registry=https://registry.npmjs.org",
    "generate-client-slovensko-sk": "ts-node scripts/generateOpenApiClient.ts slovensko-sk",
    "generate-client-city-account": "ts-node scripts/generateOpenApiClient.ts city-account",
    "generate-clients": "npm run generate-client-slovensko-sk && npm run generate-client-city-account"
  },
  "dependencies": {
    "@bratislava/ginis-sdk": "^0.5.0",
    "@golevelup/nestjs-rabbitmq": "5.7.0",
    "@golevelup/ts-jest": "0.6.2",
    "@nestjs/bull": "11.0.2",
    "@nestjs/common": "11.0.10",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.0.10",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.0.10",
    "@nestjs/schedule": "5.0.1",
    "@nestjs/swagger": "11.0.5",
    "@prisma/client": "6.4.1",
    "@rjsf/utils": "5.23.1",
    "amqplib": "^0.10.5",
    "aws-sdk": "2.1692.0",
    "axios": "1.8.1",
    "bull": "4.16.5",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "forms-shared": "file:../forms-shared",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "9.0.2",
    "jwks-rsa": "^3.1.0",
    "lodash": "4.17.21",
    "mailgun.js": "9.4.1",
    "mime-types": "2.1.35",
    "nestjs-minio-client": "^2.2.0",
    "nodemailer": "6.10.0",
    "passport-headerapikey": "^1.2.2",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.1",
    "playwright": "1.50.1",
    "reflect-metadata": "0.2.2",
    "rimraf": "^6.0.1",
    "rodnecislo": "^1.6.0",
    "uuid": "11.1.0",
    "xml2js": "^0.6.2",
    "xsd-validator": "^1.1.1"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^5.0.29",
    "@nestjs/cli": "11.0.5",
    "@nestjs/schematics": "11.0.1",
    "@nestjs/testing": "11.0.10",
    "@types/amqplib": "^0.10.6",
    "@types/express": "^5.0.0",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.15",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.13.5",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-http": "^0.3.11",
    "@types/passport-http-bearer": "^1.0.41",
    "@types/passport-jwt": "^4.0.1",
    "@types/traverse": "^0.6.37",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "clear": "^0.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-adjunct": "^4.13.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-auto": "^0.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-array-func": "^5.0.2",
    "eslint-plugin-const-case": "^1.2.2",
    "eslint-plugin-custom-rules": "file:eslint-custom-rules",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jest-async": "^1.0.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-no-unsanitized": "^4.1.2",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-pii": "^1.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-xss": "^0.1.12",
    "jest": "29.7.0",
    "jest-mock-extended": "^3.0.7",
    "minio": "^7.1.1",
    "path": "^0.12.7",
    "prettier": "^3.5.2",
    "prisma": "6.4.1",
    "prisma-json-types-generator": "3.2.2",
    "ts-jest": "29.2.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.dto.ts",
      "!**/*.module.ts",
      "!**/*.enum.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/../test/singleton.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/nases/__tests__"
    ],
    "moduleNameMapper": {
      "^react-markdown$": "<rootDir>/../test/react-markdown-mock.js"
    }
  },
  "volta": {
    "node": "20.9.0",
    "npm": "10.7.0"
  }
}

{
  "name": "cypress",
  "scripts": {
    "lint": "eslint '**/*.ts'",
    "typecheck": "tsc",
    "cypress": "./cypress.sh open chrome all",
    "cypress:open": "yarn run cypress open --e2e --browser chrome --env BASEURL=http://localhost:3000",
    "cypress:run": "yarn run cypress run --e2e --browser chrome --env BASEURL=http://localhost:3000"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "cypress": "^13.6.0",
    "eslint": "^8.54.0",
    "eslint-config-adjunct": "^4.12.2",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-prettier": "^5.0.1",
    "typescript": "5.1.3"
  },
  "engines": {
    "node": "16.x.x",
    "npm": ">=6.0.0",
    "yarn": "1.22"
  }
}

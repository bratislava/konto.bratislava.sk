{
  "name": "nest-tax-backend",
  "version": "0.0.5",
  "description": "Backend service for tax handling",
  "author": "Department of Innovation and Technology - City of Bratislava",
  "private": false,
  "license": "EUPL-1.2",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:migrate:prod": "prisma migrate deploy && npm run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,js}\"",
    "lint:report": "eslint -f json -o eslint-report.json \"{src,apps,libs,test}/**/*.{ts,js}\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.{ts,js}\" --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "vulnerabilities": "npm audit --audit-level=high --registry=https://registry.npmjs.org"
  },
  "dependencies": {
    "@nestjs-cognito/auth": "^2.2.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.12.0",
    "@types/mssql": "^9.1.7",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.10.0",
    "bysquare": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "csv-parse": "^6.1.0",
    "currency.js": "^2.0.4",
    "dayjs": "^1.11.13",
    "ejs": "^3.1.10",
    "express": "^5.0.1",
    "form-urlencoded": "^6.1.5",
    "html-pdf": "^3.0.1",
    "mssql": "^11.0.1",
    "nodemailer": "^7.0.5",
    "openapi-clients": "file:../openapi-clients",
    "passport-headerapikey": "^1.2.2",
    "qrcode": "^1.5.4",
    "rimraf": "^5.0.5",
    "ssh2-sftp-client": "^12.0.1"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^6.7.1",
    "@eslint/json": "^0.13.1",
    "@eslint/markdown": "^7.1.0",
    "@golevelup/ts-jest": "^0.7.0",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.5",
    "@types/ejs": "^3.1.5",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/express": "^5.0.1",
    "@types/html-pdf": "^3.0.3",
    "@types/jest": "30.0.0",
    "@types/node": "^20.12.7",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.5",
    "@types/ssh2-sftp-client": "^9.0.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "clear": "^0.1.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-no-unsanitized": "^4.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.4",
    "jest": "^30.0.5",
    "path": "^0.12.7",
    "prettier": "3.6.2",
    "prisma": "^6.12.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.38.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}

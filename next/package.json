  "scripts": {
    "dev": "next dev | pino-pretty -c",
    "develop": "next dev | pino-pretty -c",
    "build": "next build",
    "build:e2e": "cp .env.e2e .env.production.local && next build",
    "start": "next start",
    "typecheck": "tsc",
    "lint": "eslint assets backend clients components frontend pages",
    "lint:quiet": "yarn lint --quiet",
    "lint:fix": "yarn lint --fix",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "generate-client-forms": "openapi-generator-cli generate -i https://nest-forms-backend.staging.bratislava.sk/api-json -g typescript-axios -o ./clients/openapi-forms --skip-validate-spec && prettier --write ./clients/openapi-forms",
    "generate-client-tax": "openapi-generator-cli generate -i https://nest-tax-backend.staging.bratislava.sk/api-json -g typescript-axios -o ./clients/openapi-tax --skip-validate-spec && prettier --write ./clients/openapi-tax",
    "generate-client-city-account": "openapi-generator-cli generate -i https://nest-city-account.staging.bratislava.sk/api-json -g typescript-axios -o ./clients/openapi-city-account --skip-validate-spec && prettier --write ./clients/openapi-city-account",
    "generate-client-strapi": "cross-env CODEGEN_STRAPI_GRAPHQL_URL=https://city-account-strapi.dev.bratislava.sk/graphql graphql-codegen && prettier --write ./clients/graphql-strapi/api.ts",
    "generate-clients": "yarn generate-client-forms && yarn generate-client-tax && yarn generate-client-city-account && yarn generate-client-strapi",
    "update-react-aria": "yarn add @internationalized/date@latest @react-stately/utils@latest react-aria@latest react-aria-components@latest react-stately@latest @react-aria/utils@latest -E",
    "update-rjsf": "yarn add @rjsf/core@latest @rjsf/utils@latest @rjsf/validator-ajv8@latest ajv@latest ajv-formats@latest -E",
    "update-markdown": "yarn add remark-directive@latest remark-directive-rehype@latest remark-gfm@latest remark-supersub@latest rehype-raw@latest rehype-sanitize@latest",
    "update-next": "yarn add next@latest react@latest react-dom@latest sharp@latest -E && yarn add @types/react@latest @types/react-dom@latest eslint-config-next@latest typescript@latest -D -E",
    "update-amplify": "yarn add aws-amplify@latest @aws-amplify/adapter-nextjs@latest -E",
    "update-tailwind": "yarn add tailwind-merge@latest tailwind-scrollbar-hide@latest -E && yarn add tailwindcss@latest autoprefixer@latest postcss@latest prettier-plugin-tailwindcss@latest eslint-plugin-tailwindcss@latest -D -E",
    "update-i18next": "yarn add i18next@latest next-i18next@latest react-i18next@latest -E",
    "update-prettier": "yarn add prettier@latest eslint-config-prettier@latest -D -E",
    "update-eslint": "yarn add eslint@latest @typescript-eslint/eslint-plugin@latest @typescript-eslint/parser@latest eslint-config-adjunct@latest eslint-config-airbnb@latest eslint-config-airbnb-typescript@latest eslint-config-auto@latest eslint-config-next@latest eslint-config-prettier@latest eslint-plugin-array-func@latest eslint-plugin-const-case@latest eslint-plugin-eslint-comments@latest eslint-plugin-html@latest eslint-plugin-import@latest eslint-plugin-jest@latest eslint-plugin-jest-async@latest eslint-plugin-jest-dom@latest eslint-plugin-json@latest eslint-plugin-lodash@latest eslint-plugin-lodash-fp@latest eslint-plugin-markdown@latest eslint-plugin-no-constructor-bind@latest eslint-plugin-no-secrets@latest eslint-plugin-no-unsanitized@latest eslint-plugin-no-use-extend-native@latest eslint-plugin-optimize-regex@latest eslint-plugin-pii@latest eslint-plugin-prettier@latest eslint-plugin-promise@latest eslint-plugin-react-hooks@latest eslint-plugin-scanjs-rules@latest eslint-plugin-security@latest eslint-plugin-simple-import-sort@latest eslint-plugin-sonarjs@latest eslint-plugin-switch-case@latest eslint-plugin-tailwindcss@latest eslint-plugin-testing-library@latest eslint-plugin-unicorn@latest eslint-plugin-xss@latest @tanstack/eslint-plugin-query@latest @typescript-eslint/eslint-plugin@latest @typescript-eslint/parser@latest -D -E",
    "update-graphql": "yarn add @graphql-codegen/cli@latest @graphql-codegen/client-preset@latest @graphql-codegen/typescript-graphql-request@latest -D -E && yarn add graphql@latest graphql-request@latest graphql-tag@latest -E"
  },

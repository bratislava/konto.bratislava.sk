{
  "name": "nest-forms-backend",
  "version": "0.0.1",
  "description": "Backend responsible for processing forms for online services.",
  "author": "Department of Innovation and Technology of the City of Bratislava",
  "private": false,
  "license": "LICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "seed:prod": "node dist/prisma/seed.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,js}\"",
    "lint:report": "eslint -f json -o eslint-report.json \"{src,apps,libs,test}/**/*.{ts,js}\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.{ts,js}\" --fix",
    "test": "jest -w 1",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json",
    "test:send-form": "jest --config test/jest-send-form.json",
    "test:generate-pdf": "jest --config test/jest-generate-pdf.json",
    "vulnerabilities": "npm audit --audit-level=high --registry=https://registry.npmjs.org"
  },
  "dependencies": {
    "@bratislava/ginis-sdk": "^0.5.0",
    "@golevelup/nestjs-rabbitmq": "^5.3.0",
    "@golevelup/ts-jest": "^0.5.4",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/common": "^10.4.2",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.4.2",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.3.1",
    "@pdf-lib/fontkit": "^1.1.1",
    "@prisma/client": "^5.19.1",
    "@rjsf/utils": "5.19.3",
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1",
    "amqplib": "^0.10.4",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.6.8",
    "bull": "^4.12.2",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.10",
    "expr-eval": "^2.0.2",
    "forms-shared": "file:../forms-shared",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "libphonenumber-js": "^1.10.53",
    "mailgun.js": "^9.3.0",
    "mime-types": "^2.1.35",
    "nestjs-minio-client": "^2.2.0",
    "npm-check-updates": "^17.1.1",
    "passport-headerapikey": "^1.2.2",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.1",
    "pdf-lib": "^1.17.1",
    "playwright": "^1.45.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.7",
    "rodnecislo": "^1.6.0",
    "uuid": "^9.0.1",
    "xml2js": "^0.6.2",
    "xsd-validator": "^1.1.1"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^4.0.1",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.4.2",
    "@types/amqplib": "^0.10.5",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.9.5",
    "@types/passport-http": "^0.3.11",
    "@types/passport-http-bearer": "^1.0.41",
    "@types/passport-jwt": "^4.0.1",
    "@types/traverse": "^0.6.33",
    "@types/uuid": "^9.0.8",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.48.0",
    "clear": "^0.1.0",
    "eslint": "^8.57.0",
    "eslint-config-adjunct": "^4.13.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-auto": "^0.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-array-func": "^5.0.2",
    "eslint-plugin-const-case": "^1.2.2",
    "eslint-plugin-custom-rules": "file:eslint-custom-rules",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-jest-async": "^1.0.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-secrets": "^1.0.2",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-pii": "^1.0.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^50.0.1",
    "eslint-plugin-xss": "^0.1.12",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.4",
    "minio": "^7.1.1",
    "path": "^0.12.7",
    "prettier": "^3.3.2",
    "prisma": "^5.19.1",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.dto.ts",
      "!**/*.module.ts",
      "!**/*.enum.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/../test/singleton.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/nases/__tests__"
    ]
  }
}

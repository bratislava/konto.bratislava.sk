{
  "name": "nest-tax-backend",
  "version": "0.0.5",
  "description": "Backend service for tax handling",
  "author": "Department of Innovation and Technology - City of Bratislava",
  "private": false,
  "license": "EUPL-1.2",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:migrate:prod": "prisma migrate deploy && npm run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,js}\"",
    "lint:report": "eslint -f json -o eslint-report.json \"{src,apps,libs,test}/**/*.{ts,js}\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.{ts,js}\" --fix",
    "generate:city-account": "openapi-generator-cli generate -i https://nest-city-account.staging.bratislava.sk/api-json -g typescript-axios -o ./src/generated-clients/nest-city-account --skip-validate-spec ",
    "generate": "rm -r ./src/generated-clients/* ;npm-run-all generate:* ; prettier --write ./src/generated-clients",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "vulnerabilities": "npm audit --audit-level=high --registry=https://registry.npmjs.org"
  },
  "dependencies": {
    "@nestjs-cognito/auth": "^1.1.7",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.5",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^7.3.1",
    "@prisma/client": "^6.3.1",
    "@types/mssql": "^9.1.5",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.7",
    "bysquare": "^2.1.0",
    "class-validator": "^0.14.1",
    "currency.js": "^2.0.4",
    "ejs": "^3.1.10",
    "express": "^4.21.0",
    "form-urlencoded": "^6.1.5",
    "html-pdf": "^3.0.1",
    "mssql": "^11.0.1",
    "passport-headerapikey": "^1.2.2",
    "qrcode": "^1.5.4",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^5.0.30",
    "@golevelup/ts-jest": "^0.6.2",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.12",
    "@openapitools/openapi-generator-cli": "^2.16.3",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/html-pdf": "^3.0.3",
    "@types/jest": "29.5.14",
    "@types/node": "^20.12.7",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "clear": "^0.1.0",
    "eslint": "^8.57.1",
    "eslint-config-adjunct": "^4.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-auto": "^0.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-const-case": "^1.2.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-jest-async": "^1.0.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-no-unsanitized": "^4.1.2",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-pii": "^1.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-xss": "^0.1.12",
    "jest": "^29.7.0",
    "path": "^0.12.7",
    "prettier": "^3.5.1",
    "prisma": "^6.3.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}

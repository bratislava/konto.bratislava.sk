    "dev": "next dev --turbopack | pino-pretty -c",
    "develop": "next dev | pino-pretty -c",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:e2e": "cp .env.e2e .env.production.local && next build",
    "start": "next start",
    "typecheck": "tsc",
    "lint": "eslint assets backend clients components frontend pages",
    "lint:quiet": "npm run lint -- --quiet",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "generate-client-strapi": "graphql-codegen && prettier --write ./clients/graphql-strapi/api.ts",
    "generate-clients": "npm run generate-client-forms && npm run generate-client-tax && npm run generate-client-city-account && npm run generate-client-strapi",
    "update-react-aria": "npm install @internationalized/date@latest @react-stately/utils@latest react-aria@latest react-aria-components@latest react-stately@latest @react-aria/utils@latest --save-exact --save-exact-dev",
    "update-rjsf": "npm install @rjsf/core@latest @rjsf/utils@latest @rjsf/validator-ajv8@latest ajv@latest ajv-formats@latest --save-exact",
    "update-markdown": "npm install remark-directive@latest remark-directive-rehype@latest remark-gfm@latest remark-supersub@latest rehype-raw@latest rehype-sanitize@latest",
    "update-next": "npx @next/codemod@canary upgrade latest",
    "update-typescript": "npm install typescript@latest ts-node@latest --save-dev --save-exact",
    "update-amplify": "npm install aws-amplify@latest @aws-amplify/adapter-nextjs@latest --save-exact",
    "update-tailwind": "npm install tailwind-merge@latest clsx@latest --save-exact && npm install tailwindcss@latest @tailwindcss/postcss@latest postcss@latest prettier-plugin-tailwindcss@latest --save-dev --save-exact",
    "update-i18next": "npm install i18next@latest next-i18next@latest react-i18next@latest --save-exact",
    "update-prettier": "npm install prettier@latest eslint-config-prettier@latest --save-dev --save-exact",
    "update-eslint": "npm install eslint@latest @typescript-eslint/eslint-plugin@latest @typescript-eslint/parser@latest eslint-config-adjunct@latest eslint-config-airbnb@latest eslint-config-airbnb-typescript@latest eslint-config-auto@latest eslint-config-next@latest eslint-config-prettier@latest eslint-plugin-array-func@latest eslint-plugin-const-case@latest eslint-plugin-eslint-comments@latest eslint-plugin-html@latest eslint-plugin-import@latest eslint-plugin-jest@latest eslint-plugin-jest-async@latest eslint-plugin-jest-dom@latest eslint-plugin-json@latest eslint-plugin-lodash@latest eslint-plugin-lodash-fp@latest eslint-plugin-markdown@latest eslint-plugin-no-constructor-bind@latest eslint-plugin-no-secrets@latest eslint-plugin-no-unsanitized@latest eslint-plugin-no-use-extend-native@latest eslint-plugin-optimize-regex@latest eslint-plugin-pii@latest eslint-plugin-prettier@latest eslint-plugin-promise@latest eslint-plugin-react-hooks@latest eslint-plugin-scanjs-rules@latest eslint-plugin-security@latest eslint-plugin-simple-import-sort@latest eslint-plugin-sonarjs@latest eslint-plugin-switch-case@latest eslint-plugin-testing-library@latest eslint-plugin-unicorn@latest eslint-plugin-xss@latest @tanstack/eslint-plugin-query@latest @typescript-eslint/eslint-plugin@latest @typescript-eslint/parser@latest --save-dev --save-exact",
    "update-graphql": "npm install @graphql-codegen/cli@latest @graphql-codegen/client-preset@latest @graphql-codegen/typescript-graphql-request@latest --save-dev --save-exact && npm install graphql@latest graphql-request@latest graphql-tag@latest --save-exact",

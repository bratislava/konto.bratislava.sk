{
  "name": "nest-city-account",
  "version": "0.0.2",
  "description": "City account project.",
  "author": "Bratislava city inovation",
  "private": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:repl": "nest start --watch --entryFile repl",
    "start:prod": "node dist/main",
    "start:migrate:prod": "prisma migrate deploy && npm run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,js}\"",
    "lint:report": "eslint -f json -o eslint-report.json \"{src,apps,libs,test}/**/*.{ts,js}\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.{ts,js}\" --fix",
    "generate:new-magproxy": "openapi-generator-cli generate -i https://new-magproxy.staging.bratislava.sk/api-json -g typescript-axios -o ./src/generated-clients/new-magproxy --skip-validate-spec ",
    "generate:tax-backend": "openapi-generator-cli generate -i https://nest-tax-backend.staging.bratislava.sk/api-json -g typescript-axios -o ./src/generated-clients/nest-tax-backend --skip-validate-spec ",
    "generate": "rm -r ./src/generated-clients/* ;npm-run-all generate:* ; prettier --write ./src/generated-clients",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "vulnerabilities": "npm audit --audit-level=high --registry=https://registry.npmjs.org"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^5.7.0",
    "@nestjs/common": "^10.4.0",
    "@nestjs/core": "^10.4.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.0",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "@prisma/client": "5.19.1",
    "@types/express": "^4.17.17",
    "amqp-connection-manager": "^4.1.11",
    "amqplib": "^0.10.3",
    "aws-sdk": "^2.1354.0",
    "axios": "^1.7.4",
    "cf-turnstile": "^1.0.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto": "^1.0.1",
    "express": "^4.19.2",
    "form-data": "^4.0.0",
    "jwks-rsa": "^3.0.1",
    "lodash": "^4.17.21",
    "mailgun.js": "^8.2.1",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.1",
    "request": "^2.88.2",
    "rodnecislo": "^1.6.0",
    "rxjs": "^7.8.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^3.18.0",
    "@golevelup/ts-jest": "^0.5.0",
    "@nestjs/cli": "^10.4.4",
    "@nestjs/testing": "^10.4.0",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@types/amqplib": "^0.10.1",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.9.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/request": "^2.48.12",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^0.18.0",
    "jest": "^29.4.2",
    "jest-mock-extended": "^3.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.1",
    "prisma": "^5.19.1",
    "rimraf": "^6.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.2",
    "ts-unused-exports": "^9.0.3",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/../test/singleton.ts"
    ]
  },
  "volta": {
    "node": "20.18.2"
  }
}

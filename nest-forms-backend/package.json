    "lodash": "4.17.21",
    "mailgun.js": "^11.1.0",
    "mime-types": "3.0.1",
    "nestjs-minio-client": "^2.2.0",
    "nodemailer": "7.0.6",
    "openapi-clients": "file:../openapi-clients",
    "passport-custom": "^1.1.1",
    "passport-headerapikey": "^1.2.2",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.1",
    "piscina": "^5.1.3",
    "playwright": "1.54.2",
    "reflect-metadata": "0.2.2",
    "rimraf": "^6.0.1",
    "uuid": "11.1.0",

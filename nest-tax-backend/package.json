{
  "name": "nest-tax-backend",
  "version": "0.0.5",
  "description": "Backend service for tax handling",
  "author": "Department of Innovation and Technology - City of Bratislava",
  "private": false,
  "license": "EUPL-1.2",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:migrate:prod": "prisma migrate deploy && npm run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,js}\"",
    "lint:report": "eslint -f json -o eslint-report.json \"{src,apps,libs,test}/**/*.{ts,js}\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.{ts,js}\" --fix",
    "test": "jest --passWithNoTests --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "vulnerabilities": "npm audit --audit-level=high --registry=https://registry.npmjs.org"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.917.0",
    "@nestjs-cognito/auth": "^2.5.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@prisma/client": "6.17.1",
    "@types/mssql": "^9.1.8",
    "axios": "^1.12.2",
    "bysquare": "2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "csv-parse": "^6.1.0",
    "currency.js": "^2.0.4",
    "dayjs": "^1.11.18",
    "ejs": "^3.1.10",
    "express": "^5.0.1",
    "form-urlencoded": "^6.1.6",
    "html-pdf": "^3.0.1",
    "mssql": "^12.0.0",
    "nodemailer": "^7.0.6",
    "openapi-clients": "file:../openapi-clients",
    "p-limit": "3.1.0",
    "passport-headerapikey": "^1.2.2",
    "qrcode": "^1.5.4",
    "rimraf": "^5.0.5",
    "ssh2-sftp-client": "^12.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "5.1.4",
    "@golevelup/ts-jest": "^0.7.0",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.6",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.4",
    "@types/html-pdf": "^3.0.3",
    "@types/jest": "30.0.0",
    "@types/node": "^20.12.7",
    "@types/nodemailer": "^7.0.2",
    "@types/qrcode": "^1.5.5",
    "@types/ssh2-sftp-client": "^9.0.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "clear": "^0.1.0",
    "eslint": "^8.57.1",
    "eslint-config-adjunct": "^4.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-auto": "^0.9.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-const-case": "^1.2.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.13.3",
    "eslint-plugin-jest-async": "^1.0.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-no-unsanitized": "^4.1.2",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-pii": "^1.0.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-xss": "^0.1.12",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "path": "^0.12.7",
    "prettier": "^3.5.2",
    "prisma": "6.17.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}

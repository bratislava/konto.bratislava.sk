{
  "name": "nest-clamav-scanner",
  "version": "0.0.2",
  "description": "NestJS ClamAV Scanner using ClamAV.js for scanning files and streams.",
  "author": "Department of Innovation and Technology of the City of Bratislava",
  "private": false,
  "license": "LICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:migrate:prod": "prisma migrate deploy && npm run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,js}\"",
    "lint:report": "eslint -f json -o eslint-report.json \"{src,apps,libs,test}/**/*.{ts,js}\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.{ts,js}\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "vulnerabilities": "npm audit --audit-level=high --registry=https://registry.npmjs.org"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.11",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.11",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.11",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.0.6",
    "@prisma/client": "^6.4.1",
    "@types/passport-http-bearer": "^1.0.41",
    "@types/uuid": "^10.0.0",
    "await-exec": "^0.1.2",
    "axios": "1.8.1",
    "base64url": "^3.0.1",
    "clamdjs": "^1.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "husky": "^9.1.7",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "mime-types": "^2.1.35",
    "minio": "^8.0.5",
    "nestjs-minio-client": "^2.2.0",
    "openapi-clients": "file:../openapi-clients",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^6.4.1",
    "rimraf": "^6.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^6.4.2",
    "@eslint/js": "^9.21.0",
    "@eslint/json": "^0.11.0",
    "@eslint/markdown": "^6.3.0",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.2",
    "@nestjs/testing": "^11.0.11",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "29.5.14",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.13.9",
    "@types/passport-http": "^0.3.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jest-async": "^1.0.3",
    "eslint-plugin-no-unsanitized": "^4.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-xss": "^0.1.12",
    "jest": "^29.7.0",
    "jiti": "^2.4.2",
    "path": "^0.12.7",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0"
  },
  "overrides": {
    "glob": "^11.0.1"
  },
  "volta": {
    "node": "22.14.0",
    "npm": "11.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}

    "@nestjs/schematics": "11.0.7",
    "@nestjs/testing": "11.1.6",
    "@ngneat/falso": "^8.0.2",
    "@testcontainers/postgresql": "^11.5.1",
    "@types/amqplib": "^0.10.7",
    "@types/content-disposition": "^0.5.9",
